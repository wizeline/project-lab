{
  "name": "proposalHunt",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "sync-skills": "ts-node --project ./ts-node.tsconfig.json ./tasks/syncSkills.ts",
    "sync-jobTitles": "ts-node --project ./ts-node.tsconfig.json ./tasks/syncJobTitles.ts",
    "sync-locations": "ts-node --project ./ts-node.tsconfig.json ./tasks/syncLocations.ts",
    "pm2:server": "pm2 start pm2-server.json",
    "pm2:db-replication": "pm2 start pm2-db-replication.json"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@material-ui/core": "5.0.0-beta.2",
    "@prisma/client": "2.25.0",
    "axios": "0.21.1",
    "blitz": "0.38.1",
    "final-form": "4.20.2",
    "lodash.debounce": "4.0.8",
    "passport-auth0": "1.4.0",
    "pm2": "5.1.0",
    "react": "alpha",
    "react-dom": "alpha",
    "react-final-form": "6.5.3",
    "zod": "3.2.0"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.11",
    "eslint": "7.29.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.3.1",
    "prettier-plugin-prisma": "0.12.1",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.4",
    "prisma": "2.25.0",
    "typescript": "~4.3"
  },
  "private": true
}
